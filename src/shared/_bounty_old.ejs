<%
hasCampaign = function (bounty, weekNo, campaignName) {
let result = false;

const week = bounty.weeks.find(week => week.parsedWeekNo == weekNo);
if (week) {
const campaign = week.campaigns[campaignName];
if (campaign) {
result = campaign.done;
}
}

let output = '';
if (result) {
output = 'bounty-done';
}

return output;
}
%>


